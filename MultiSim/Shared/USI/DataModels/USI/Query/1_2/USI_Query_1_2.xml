<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
<usi:tdm version="1.0" xmlns:usi="http://www.ni.com/Schemas/USI/1_0">

  <usi:documentation>
    <usi:exporter>National Instruments USI</usi:exporter>
    <usi:exporterVersion>1.1</usi:exporterVersion>
  </usi:documentation>

  <usi:model modelName="National Instruments USI generated meta file" modelVersion="1.0">
    <usi:include nsUri="http://www.ni.com/DataModels/USI/1_0"/>
    <usi:enumeration name="t_operators">
      <usi:item name="equal"   value="0"/>
      <usi:item name="not_equal"  value="1"/>
      <usi:item name="greater"   value="2"/>
      <usi:item name="greater_than"  value="3"/>
      <usi:item name="less"   value="4"/>
      <usi:item name="less_than"  value="5"/>
      <usi:item name="after"  value="6"/>
      <usi:item name="before"  value="7"/>
      <usi:item name="day" value="8"/>
      <usi:item name="pre" value="9"/>
    </usi:enumeration>
    
    <usi:enumeration name="t_entities">
      <usi:item name="file"          value="0"/>
      <usi:item name="channelgroup"  value="1"/>
      <usi:item name="channel"       value="2"/>
	  <usi:item name="as_entity"       value="4"/>
    </usi:enumeration>
    
    <usi:enumeration name="t_orderBy">
      <usi:item name="eNoOrderBy"          value="0"/>
      <usi:item name="eOrderByAscending"  value="1"/>
      <usi:item name="eOrderByDescending"       value="2"/>
    </usi:enumeration>
	
	<usi:enumeration name="t_ResultsHdrMode">
			<usi:item name="eResultsHdrModeLevelProperty" value="1"/>
			<usi:item name="eResultsHdrModeProperty" value="2"/>
	</usi:enumeration>
    
    <usi:entity derivation="redeclaration" name="query_manager" superTypes="aoany">
      <usi:attribute base="name" name="name">
      	<usi:value>query_manager</usi:value>
      </usi:attribute>
      <usi:reference maxOccurs="unbounded" minOccurs="0" inverse="manager" name="queries" reference="query" refType="composition"/>
    </usi:entity>
	
	<usi:entity derivation="extension" name="asamquery_manager" superTypes="query_manager">
      
    </usi:entity>
	   
     <usi:entity derivation="redeclaration" name="query" superTypes="aoany">
      <usi:attribute base="name" name="name">
      	<usi:value>query_manager</usi:value>
      </usi:attribute>
      <usi:attribute base="version" name="version"/>
      <usi:reference maxOccurs="unbounded" minOccurs="0" inverse="queries" name="manager" reference="query_manager"/>
    </usi:entity>
    
    <usi:entity derivation="extension" name="textquery" superTypes="query">
      <usi:attribute maxOccurs="1" minOccurs="1" name="text" type="eStringUsi"/>
    </usi:entity>

    <usi:entity derivation="extension" name="asamtextquery" superTypes="textquery">
    </usi:entity>
    
    <usi:entity derivation="extension" name="advancedquery" superTypes="query">
      <usi:attribute maxOccurs="1" minOccurs="1" name="returntype" type="eEnumUsi" reference="t_entities"/>
      <usi:reference maxOccurs="unbounded" minOccurs="0" inverse="query" name="conditions" reference="condition" refType="composition"/>
      <usi:attribute maxOccurs="1" minOccurs="1" name="logic" type="eStringUsi"/>
    </usi:entity>
	
	<usi:entity derivation="extension" name="asamquery" superTypes="advancedquery">
      <usi:attribute maxOccurs="1" minOccurs="1" name="returntype_name" type="eStringUsi"/>
	  <usi:attribute maxOccurs="1" minOccurs="1" name="filterActive" type="eInt16Usi">
        <usi:value>0</usi:value>
      </usi:attribute>
    </usi:entity>
    
    <usi:entity derivation="redeclaration" name="condition" superTypes="aoany">
      <usi:attribute base="name" name="name">
      	<usi:value>condition</usi:value>
      </usi:attribute>
      <usi:attribute maxOccurs="1" minOccurs="1" name="type" type="eEnumUsi" reference="t_entities"/>
	  <usi:attribute maxOccurs="1" minOccurs="1" name="property" type="eStringUsi"/>
      <usi:attribute maxOccurs="1" minOccurs="1" name="operator" type="eEnumUsi" reference="t_operators"/>
      <usi:attribute maxOccurs="1" minOccurs="1" name="value" type="eStringUsi"/>
      <usi:attribute maxOccurs="1" minOccurs="1" name="values" type="eStringUsi"/>
      <usi:attribute maxOccurs="1" minOccurs="1" name="orderBy" type="eEnumUsi" reference="t_orderBy">
        <usi:value>eNoOrderBy</usi:value>
      </usi:attribute>
	  <usi:attribute maxOccurs="1" minOccurs="1" name="datatype" type="eInt32Usi">
		<usi:value>0</usi:value>
	  </usi:attribute>
      <usi:reference maxOccurs="1" minOccurs="1" inverse="conditions" name="query" reference="advancedquery"/>
    </usi:entity>
	
	<usi:entity derivation="extension" name="asamcondition" superTypes="condition">
      <usi:attribute maxOccurs="1" minOccurs="1" name="type_name" type="eStringUsi" />
    </usi:entity>
	
	<usi:entity derivation="redeclaration" name="column_manager" superTypes="aoany">
      <usi:attribute base="name" name="name">
      	<usi:value>column_manager</usi:value>
      </usi:attribute>
      <usi:reference maxOccurs="unbounded" minOccurs="0" inverse="manager" name="column_configs" reference="column_config" refType="composition"/>
    </usi:entity>
	
	<usi:entity derivation="extension" name="asamcolumn_manager" superTypes="column_manager">
      
    </usi:entity>
	
	<usi:entity derivation="redeclaration" name="column_config" superTypes="aoany">
      <usi:attribute base="name" name="name">
      	<usi:value>column_config</usi:value>
      </usi:attribute>
	  <usi:attribute base="version" name="version"/>
	  <usi:attribute maxOccurs="1" minOccurs="1" name="automatic_columns" type="eInt16Usi" />
	  <usi:attribute maxOccurs="1" minOccurs="1" name="colHdrMode" type="eEnumUsi" reference="t_ResultsHdrMode">
	    <usi:value>eResultsHdrModeLevelProperty</usi:value>
	  </usi:attribute>
	  <usi:reference maxOccurs="unbounded" minOccurs="1" inverse="column_config" name="returntype_configs" reference="returntype_config" refType="composition"/>
      <usi:reference maxOccurs="1" minOccurs="1" inverse="column_configs" name="manager" reference="column_manager"/>
    </usi:entity>
	
	<usi:entity derivation="extension" name="asamcolumn_config" superTypes="column_config">
      
    </usi:entity>
	
	<usi:entity derivation="redeclaration" name="returntype_config" superTypes="aoany">
	   <usi:attribute base="name" name="returntype_name"/>
	   <usi:attribute maxOccurs="1" minOccurs="1" name="first_col_width" type="eInt32Usi" />
	   <usi:reference maxOccurs="unbounded" minOccurs="0" inverse="returntype_config" name="columns" reference="column" refType="composition"/>
	   <usi:reference maxOccurs="1" minOccurs="1" inverse="returntype_configs" name="column_config" reference="column_config"/>
	</usi:entity>
	
	<usi:entity derivation="extension" name="asamreturntype_config" superTypes="returntype_config">
      
    </usi:entity>
	
	<usi:entity derivation="redeclaration" name="column" superTypes="aoany">
      <usi:attribute base="name" name="name">
      	<usi:value>column</usi:value>
      </usi:attribute>
      <usi:attribute maxOccurs="1" minOccurs="1" name="type_name" type="eStringUsi" />
	  <usi:attribute maxOccurs="1" minOccurs="1" name="property" type="eStringUsi" />
	  <usi:attribute maxOccurs="1" minOccurs="1" name="width" type="eInt32Usi" />
	  <usi:attribute maxOccurs="1" minOccurs="1" name="datatype" type="eInt32Usi">
		<usi:value>0</usi:value>
	  </usi:attribute>
      <usi:reference maxOccurs="1" minOccurs="1" inverse="columns" name="returntype_config" reference="returntype_config"/>
    </usi:entity>
	
	<usi:entity derivation="extension" name="asamcolumn" superTypes="column">
      
    </usi:entity>
    
  </usi:model>

  <usi:data/>

</usi:tdm>
