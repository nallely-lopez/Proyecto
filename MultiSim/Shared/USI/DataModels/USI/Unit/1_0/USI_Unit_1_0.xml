<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
<usi:tdm usi:version="1.0" xmlns:usi="http://www.ni.com/Schemas/USI/1_0">
  <usi:model usi:modelname="unitcatalog" usi:modelversion="1.0">
    <usi:include nsUri="http://www.ni.com/DataModels/USI/1_0"/>
        
    <usi:enumeration name="scale_type_enum">
      <usi:item name="linear" value="0"/>
      <usi:item name="linearoffset" value="1"/>
      <usi:item name="logarithmic" value="2"/>
      <usi:item name="unknown" value="3"/>
      <usi:item name="reserved1" value="4"/>
      <usi:item name="reserved2" value="5"/>
      <usi:item name="reserved3" value="6"/>
    </usi:enumeration>
    
    <usi:entity derivation="redeclaration" name="tdm_unit_catalog" superTypes="aoany">
      <usi:attribute base="name" name="name"/>
      <usi:attribute base="description" name="description"/>
      <usi:reference maxOccurs="unbounded" minOccurs="0" inverse="catalog" name="unit_sets" reftype="composition" reference="tdm_unit_set"/>
      <usi:reference maxOccurs="1" minOccurs="0" inverse="default_in_catalog" name="default_unit_set" reftype="aggregation" reference="tdm_unit_set"/>
      <usi:attribute maxOccurs="1" minOccurs="0" name="default_unit_set_reftxt" type="eStringUsi"/>
      <usi:reference maxOccurs="unbounded" minOccurs="0" inverse="catalog" name="phys_quantities" reftype="composition" reference="tdm_phys_quantity"/>
    </usi:entity>
    
    <usi:entity derivation="redeclaration" name="tdm_unit_set" superTypes="aoany">
      <usi:attribute base="name" name="name"/>
      <usi:attribute base="description" name="description"/>
      <usi:attribute maxOccurs="1" minOccurs="1" name="displayname" type="eStringUsi"/>
      <usi:reference maxOccurs="unbounded" minOccurs="0" inverse="unit_set" name="set_quantities" reftype="composition" reference="tdm_set_quantity"/>
      <usi:reference maxOccurs="1" minOccurs="0" inverse="unit_sets" name="catalog" reftype="aggregation" reference="tdm_unit_catalog"/>
      <usi:reference maxOccurs="1" minOccurs="0" inverse="default_unit_set" name="default_in_catalog" reftype="aggregation" reference="tdm_unit_catalog"/>
    </usi:entity>
    
    <usi:entity derivation="redeclaration" name="tdm_set_quantity" superTypes="aounitgroup">
      <usi:attribute base="name" name="name"/>
      <usi:reference base="units" inverse="set_quantities" name="units" reference="tdm_unit"/>
      <usi:attribute maxOccurs="unbounded" minOccurs="0" name="units_reftxt" type="eStringUsi"/>
      <usi:reference maxOccurs="1" minOccurs="0" inverse="default_in_set_quantities" name="default_unit" reference="tdm_unit"/>
      <usi:attribute maxOccurs="1" minOccurs="0" name="default_unit_reftxt" type="eStringUsi"/>
      <usi:reference maxOccurs="1" minOccurs="0" inverse="set_quantities" name="unit_set" reftype="aggregation" reference="tdm_unit_set"/>
      <usi:reference maxOccurs="1" minOccurs="1" inverse="set_quantities" name="phys_quantity" reference="tdm_phys_quantity"/>
    </usi:entity>
    
    <usi:entity derivation="redeclaration" name="tdm_unit" superTypes="aounit">
      <usi:attribute base="name" name="name"/>
      <usi:attribute base="description" name="description"/>
      <usi:attribute maxOccurs="1" minOccurs="1" name="displayname" type="eStringUsi"/>
      <usi:reference maxOccurs="unbounded" minOccurs="0" inverse="unit" name="symbols" reftype="composition" reference="tdm_unit_symbol"/>
      <usi:reference maxOccurs="1" minOccurs="0" inverse="default_in_unit" name="default_symbol" reftype="aggregation" reference="tdm_unit_symbol"/>
      <usi:attribute maxOccurs="1" minOccurs="0" name="default_symbol_reftxt" type="eStringUsi"/>
      <usi:attribute maxOccurs="1" minOccurs="1" name="scale_type" reference="scale_type_enum" type="eEnumUsi">
        <usi:value>linear</usi:value>
      </usi:attribute>
      <usi:attribute base="factor" name="factor"><usi:value>1</usi:value></usi:attribute>
      <usi:attribute base="offset" name="offset"><usi:value>0</usi:value></usi:attribute>
      <usi:reference base="groups" inverse="units" name="set_quantities" reference="tdm_set_quantity"/>
      <usi:reference maxOccurs="unbounded" minOccurs="0" inverse="default_unit" name="default_in_set_quantities" reference="tdm_set_quantity"/>
      <usi:reference maxOccurs="1" minOccurs="1" inverse="units" name="phys_quantity" reftype="aggregation" reference="tdm_phys_quantity"/>
    </usi:entity>
    
    <usi:entity derivation="redeclaration" name="tdm_unit_symbol" superTypes="aoany">
      <usi:attribute base="name" name="name"/>
      <usi:attribute base="description" name="description"/>
      <usi:reference maxOccurs="1" minOccurs="1" inverse="symbols" name="unit" reftype="aggregation" reference="tdm_unit"/>
      <usi:reference maxOccurs="1" minOccurs="0" inverse="default_symbol" name="default_in_unit" reftype="aggregation" reference="tdm_unit"/>
    </usi:entity>
    
    <usi:entity derivation="redeclaration" name="tdm_phys_quantity" superTypes="aophysicaldimension">
      <usi:attribute base="name" name="name"/>
      <usi:attribute base="description" name="description"/>
      <usi:attribute maxOccurs="1" minOccurs="1" name="displayname" type="eStringUsi"/>
      <usi:reference base="units" inverse="phys_quantity" name="units" reftype="composition" reference="tdm_unit"/>
      <usi:attribute base="length_exp" name="length_exp"/>
      <usi:attribute base="mass_exp" name="mass_exp"/>
      <usi:attribute base="time_exp" name="time_exp"/>
      <usi:attribute base="current_exp" name="current_exp"/>
      <usi:attribute base="temperature_exp" name="temperature_exp"/>
      <usi:attribute base="molar_amount_exp" name="molar_amount_exp"/>
      <usi:attribute base="luminous_intensity_exp" name="luminous_intensity_exp"/>
      <usi:attribute maxOccurs="1" minOccurs="0" name="category" type="eStringUsi"/>
      <usi:reference maxOccurs="unbounded" minOccurs="0" inverse="phys_quantity" name="set_quantities" reference="tdm_set_quantity"/>
      <usi:reference maxOccurs="1" minOccurs="0" inverse="phys_quantities" name="catalog" reftype="aggregation" reference="tdm_unit_catalog"/>
    </usi:entity>
    
  </usi:model>

</usi:tdm>
