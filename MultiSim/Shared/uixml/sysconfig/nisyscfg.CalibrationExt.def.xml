<?xml version="1.0" encoding="utf-8"?>
<item xmlns:v14="http://www.ni.com/uixml/item/2014">
	<rules>
		<if>
			<condition>
				<!-- mxSysAttrTag_CalExtSupportsWrite -->
				<eq sysattr="17215488" value="true"/>
			</condition>
			<setprop id="extcal.lastcal" writable="true"/>
			<setprop id="extcal.nextcal" writable="true" caption="{extcalduedate}"/>
			<setprop id="extcal.recommendedint" visible="true"/>
			<setcmd id="extcal.changecalpassword" visible="true"/>
		</if>
	</rules>
	<group id="extcal" caption="{extcalibration}" order="100" weight="0">
		<property id="lastcal" caption="{lastcalibrated}" sysattr="16863232" order="100" delayed="true" visible="always" placeholder="{na}"/>
		<property id="nextcal" caption="{recommendednext}" sysattr="16871424" order="200" delayed="true" visible="always" placeholder="{na}"/>
		<property id="recommendedint" caption="{recommendedint}" sysattr="17207296" order="300" delayed="true" visible="false" placeholder="{na}" undefinedif="-1">
			<numberformat units="{months}" precision="0"/>
		</property>
		<property id="temperature" caption="{caltemperature}" sysattr="16867328" order="400" delayed="true" visible="always" placeholder="{na}">
			<numberformat units="{degC}" precision="1"/>
		</property>
		<property id="comments" caption="{calcomments}" sysattr="16961536" order="500" delayed="true" visible="true" undefinedif=""/>
		<v14:command id="changecalpassword" caption="{changecalpassword}" visible="false"/>
	</group>
</item>
